    <script>
      console.log(`
    __  ___ ___    ______
   /  |/  //   |  / ____/
  / /|_/ // /| | / /     
 / /  / // ___ |/ /___   
/_/  /_//_/  |_|\____/   
                         
          `)
    </script>
    
  </head>

  <body>
 

    <div class="main">

      <% if flash[:errors] %>
        <%= render 'layouts/errors' %>
      <% end %>

      <% if logged_in? %>
        <%= render 'layouts/navbar' %>
        <%= render 'layouts/vehicle_tabs' %>
      <% else %> 
        <%= yield %>
      <% end %>

    </div>

  </body>
</html>
